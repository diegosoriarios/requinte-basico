<%- include('header') %>
    <h2>Open roles:</h2>
    <% categorias.forEach( categoria => { %>
    <div>
        <h3><%- categoria.categoria %></h3>
        <ul>
            <% categoria.produtos.forEach( produto => { %>
                <li><a href="produto/<%- produto.id %>"><%- produto.titulo %></a></li>
            <% }) %>
        </ul>
    </div>
    <% }) %>
<%- include('footer') %>
